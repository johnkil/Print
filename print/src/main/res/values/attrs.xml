<resources>

    <declare-styleable name="PrintView">
        <!-- Icon. -->
        <attr name="iconText" format="string"/>
        <!-- Icon color. -->
        <attr name="iconColor" format="color"/>
        <!-- Size of the icon. -->
        <attr name="iconSize" format="dimension"/>
        <!-- Icon font name. -->
        <attr name="fontName" format="string"/>
    </declare-styleable>

</resources>