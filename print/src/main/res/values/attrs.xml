<resources>

    <declare-styleable name="PrintView">
        <!-- Icon text. -->
        <attr name="print_iconText" format="string"/>
        <!-- Icon text based on char code. -->
        <attr name="print_iconCode" format="integer"/>
        <!-- Color of the icon. -->
        <attr name="print_iconColor" format="color"/>
        <!-- Size of the icon. -->
        <attr name="print_iconSize" format="dimension"/>
        <!-- File name of the font in the assets directory. -->
        <attr name="print_iconFont" format="string"/>

        <!-- Icon text for button. -->
        <attr name="print_iconTextLeft" format="string"/>
        <attr name="print_iconTextRight" format="string"/>
        <attr name="print_iconTextStart" format="string"/>
        <attr name="print_iconTextEnd" format="string"/>
        <attr name="print_iconTextTop" format="string"/>
        <attr name="print_iconTextBottom" format="string"/>
    </declare-styleable>

</resources>